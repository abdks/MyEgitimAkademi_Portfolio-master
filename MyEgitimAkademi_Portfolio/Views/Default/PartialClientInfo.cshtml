@model List<MyEgitimAkademi_Portfolio.Models.ClientInfo>
<section class="w3l-homeblock3" id="clients">
    <div class="midd-w3 py-5">
        <div class="container py-lg-5 py-md-3">
            <div class="row">
                <div class="col-lg-6">
                    <span class="text">Müşterilerim ve Partnerlerim </span>
                    @foreach (var item in Model)
                    {
                        <h3 class="title-big">
                            @item.SubjectText
                        </h3>
                    
                        <p class="mt-4">
                            @item.DescriptionText
                        </p>
                      }
                    @Html.Action("PartialClientStats")
                        
                    </div>
                @Html.Action("PartialClients")
            </div>
        </div>
    </div>
</section>
