@model List<MyEgitimAkademi_Portfolio.Models.ClientStats>
<div class="reward-list mt-5">
    @foreach (var item in Model)
    {
        <div class="reward-item">
            <div class="reward-item-photo">
                @item.StatsValue
            </div>
            <div class="reward-item-detail">
                <span class="reward-item-title">@item.SubjectText</span>
                <p class="reward-item-descr">@item.DescriptionText</p>
            </div>
        </div>
    }
</div>

